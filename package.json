{
  "name": "svelte.technology",
  "version": "1.0.0",
  "description": "Docs and examples for Svelte",
  "scripts": {
    "start": "node server",
    "bundle": "rollup -c",
    "prebundle": "rm -rf client/dist && mkdir -p client/dist && rm -rf service-worker/dist && mkdir -p service-worker/dist",
    "dev": "export DEV=true && rollup -c -w & node scripts/prep && node server",
    "prep": "npm run zip && node scripts/prep && npm run bundle",
    "zip": "sh scripts/zip.sh",
    "prestage": "npm run prep",
    "predeploy": "npm run prep",
    "predeploy:ci": "npm run prep",
    "stage": "now",
    "deploy": "now && now alias",
    "deploy:ci": "now --token $NOW_TOKEN && now alias --token $NOW_TOKEN",
    "lint": "eslint server client shared"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sveltejs/svelte.technology.git"
  },
  "keywords": [
    "svelte"
  ],
  "author": "Rich Harris",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sveltejs/svelte.technology/issues"
  },
  "homepage": "https://github.com/sveltejs/svelte.technology#readme",
  "devDependencies": {
    "chalk": "^1.1.3",
    "clean-css": "^4.0.12",
    "compression": "^1.6.2",
    "eslint": "^4.0.0",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-import": "^2.2.0",
    "express": "^4.15.2",
    "glob": "^7.1.1",
    "highlight.js": "^9.11.0",
    "html-entities": "^1.2.1",
    "marked": "^0.3.6",
    "now": "^6.2.0",
    "rollup": "^0.42.0",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-hash": "^1.2.0",
    "rollup-plugin-json": "^2.1.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-svelte": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.0.0",
    "uglify-js": "^3.0.0"
  },
  "dependencies": {
    "codemirror": "^5.25.2",
    "curl-amd": "^0.8.12",
    "file-saver": "^1.3.3",
    "jszip": "^3.1.3",
    "locate-character": "^2.0.0",
    "roadtrip": "^0.5.1",
    "rollup": "^0.42.0",
    "sourcemap-codec": "^1.3.1",
    "svelte": "^1.21.0"
  },
  "now": {
    "alias": "svelte.technology",
    "files": [
      "public",
      "server",
      "client/dist",
      "service-worker/dist"
    ]
  }
}
